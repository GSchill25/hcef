<h5>Instructor Sign Up</h5>
<% if @instructor.errors.any? %>
    <div id="error_explanation">
      <h4><%= pluralize(@instructor.errors.count, "error") %> prohibited this instructor from being saved:</h4>

      <ul>
      <% @instructor.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
<% end %>
<%= nested_form_for(@instructor, :url=>{:action=>"create"}, html: {class: ""}) do |f| %>
				        	<%= f.text_field :first_name%>
				        	<label for="icon_prefix">First Name</label>
					    	<%= f.text_field :last_name %>
					    	<label for="icon_prefix">Last Name</label>
						<%= f.fields_for :user do |users| %>
							<%= render :partial => "user_form", :locals => { :f => users } %>
						<% end %>
							<button class="" type="submit" name="action">Submit
							</button>
<% end %>
