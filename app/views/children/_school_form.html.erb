</br>
					<%= form_for(@school, :url=>{:controller=>"schools", :action=>"create"}, html: {class: ""}) do |f| %>
				        		<%= f.text_field :name, html: {id: "icon_prefix"} %>
				        		<label for="icon_prefix">Name</label>
						        <%= f.text_field :phone, html: {id: "icon_telephone"} %>
						        <label for="icon_telephone">Phone</label>
						        <%= f.text_field :address_line_one %>
						        <label for="addr_one">Address Line One</label>
						        <%= f.text_field :address_line_two %>
						        <label for="addr_one">Address Line Two</label>
						        <%= f.text_field :city %>
						        <label for="city">City</label>
						        <%= f.text_field :state %>
						        <label for="state">State</label>
						        <%= f.text_field :zip %>
						        <label for="zip">Zip Code</label>
								  <%= f.submit "Create", html: {class: ""} %>
					<% end %>
