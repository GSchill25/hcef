      <table class="table table-striped">
      <tr>
        <thead>
          <th>
            Date
          </th>
          <th>
            Student
          </th>
          <th>
            Tech Time
          </th>
          <th>
            Literature Time
          </th>
          <th>
            Homework Time
          </th>
          <th>
            Reading Time
          </th>
          <th>
            Total Minutes
          </th>
        </thead>
      </tr>
        <% @days.each do |d| %>
	      <tr>
	        <td><%= d.date %></td>
	        <td><%= d.child.name %></td>
	        <td><%= d.technology_time %></td>
	        <td><%= d.literacy_time %></td>
	        <td><%= d.homework_time %></td>
	        <td><%= d.reading_specialist_time %></td>
	        <td><%= d.total_hours %></td>
	      </tr>
	    <% end %>
      </table>



